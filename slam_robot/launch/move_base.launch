<launch>
  <master auto="start"/>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find slam_robot)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find slam_robot)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find slam_robot)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find slam_robot)/params/static_global_costmap_params.yaml" command="load" />
    <rosparam file="$(find slam_robot)/params/planner_params.yaml" command="load" />
    <param name="base_local_planner" value="trajectory_planner_diff/TrajectoryPlannerDiffROS" />
    <param name="controller_frequency" value="15">
    <remap from="/cmd_vel" to="/RosAria/cmd_vel"/>

  </node>
</launch>
